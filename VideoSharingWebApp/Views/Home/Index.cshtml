@{
    ViewBag.Title = "Home Page";
}

<h2>Upload Your Video</h2>
@using (Html.BeginForm("Index",
                        "Home",
                        FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
{
    <input type="file" name="file" id="file" /><br />
    <input type="submit" value="Upload Image"/>
    @ViewBag.Message
}


  